{
  "name": "pyxt",
  "displayName": "PyXT",
  "description": "Python eXTensions for VS Code",
  "author": "Daniel Miller",
  "repository": "https://github.com/millerdev/pyxt",
  "license": "SEE LICENSE.txt",
  "homepage": "https://github.com/millerdev/pyxt",
  "version": "0.1.0",
  "publisher": "millerdev",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:pyxt.command",
    "onCommand:pyxt.ag",
    "onCommand:pyxt.isort",
    "onCommand:pyxt.open",
    "onCommand:pyxt.python"
  ],
  "contributes": {
    "commands": [
      {
        "command": "pyxt.command",
        "title": "PyXT: Command"
      },
      {
        "command": "pyxt.ag",
        "title": "PyXT: Ag (The Silver Searcher)"
      },
      {
        "command": "pyxt.isort",
        "title": "PyXT: isort"
      },
      {
        "command": "pyxt.open",
        "title": "PyXT: Open File"
      },
      {
        "command": "pyxt.python",
        "title": "PyXT: Python"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "pyxt",
      "properties": {
        "pyxt.pythonPath": {
          "type": "string",
          "default": "",
          "description": "Python interpreter path. Recommended: create a virtualenv with Python 3.7+ and set this to the absolute path to its Python executable."
        },
        "pyxt.agPath": {
          "type": "string",
          "default": "ag",
          "description": "Ag (The Silver Searcher) executable path."
        },
        "pyxt.userScript": {
          "type": "string",
          "default": null,
          "description": "File path or importable Python module path referencing user-defined commands. See 'Adding your own custom commands' in the PyXT README for more details."
        }
      }
    }
  },
  "main": "./client/extension",
  "scripts": {
    "test": "mocha client/test",
    "pkg": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.34.0",
    "mocha": "^8.2.1",
    "proxyquire": "^2.1.3",
    "sinon": "^9.2.1",
    "vsce": "^1.81.1",
    "vscode-test": "^1.4.1"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "vscode-languageclient": "^5.2.1"
  }
}
