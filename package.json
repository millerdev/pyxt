{
  "name": "vsxt",
  "description": "VS Code XT Commands",
  "author": "Daniel Miller",
  "repository": "https://github.com/millerdev/vsxt",
  "license": "Apache-2.0",
  "version": "0.0.1",
  "publisher": "millerdev",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vsxt.command",
    "onCommand:vsxt.openFile"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vsxt.command",
        "title": "XT: Command"
      },
      {
        "command": "vsxt.openFile",
        "title": "XT: Open File"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "XT Server Configuration",
      "properties": {}
    }
  },
  "main": "./client/extension",
  "scripts": {
    "test": "mocha client/test"
  },
  "devDependencies": {
    "@types/vscode": "^1.51.0",
    "mocha": "^8.2.1",
    "sinon": "^9.2.1",
    "vscode-test": "^1.4.1"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "vscode-languageclient": "^5.2.1"
  }
}
